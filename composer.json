{
    "name": "rela589n/runtime-injection",
    "description": "Framework agnostic runtime dependency injection wrapper, allowing to register any framework-specific container adapter.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rela589n\\RuntimeInjection\\": "src/"
        },
        "files": [
            "src/inject.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rela589n\\RuntimeInjection\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "rela589n",
            "email": "zsidelnik@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2 || ^8.0",
        "webmozart/assert": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "vimeo/psalm": "^4.23"
    }
}
